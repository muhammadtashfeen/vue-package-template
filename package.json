{
  "name": "@mtashfeen/vue-package-template",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-package-template.umd.js",
  "module": "./dist/vue-package-template.es.js",
